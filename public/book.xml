<?xml version="1.0" encoding="UTF-8"?>

<!-- Example of book -->
<ns1:book
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns:ns1='http://it.bmc.uu.se/compdept/slimbook'
    xsi:schemaLocation='http://it.bmc.uu.se/compdept/slimbook slimbook.xsd'>
    <!-- Book information -->
    <ns1:info>
        <ns1:title></ns1:title>
        <ns1:date>1970-01-01</ns1:date>
        <ns1:version></ns1:version>
        <ns1:author>
            <ns1:name></ns1:name>
            <ns1:email></ns1:email>
        </ns1:author>
    </ns1:info>

    <!-- Chapters can have zero or more paragraphs -->
    <ns1:chapter title="" name="">
    </ns1:chapter>
    
    <ns1:chapter title="" name="">
        <ns1:paragraph title="" name="">
        </ns1:paragraph>
    </ns1:chapter>    
    
    <ns1:chapter title="" name="">
        <ns1:paragraph title="" name="">
            <ns1:video source="" title=""></ns1:video>
            <ns1:image source="" title=""></ns1:image>
            <ns1:link target="" title=""></ns1:link>
        </ns1:paragraph>
        
        <ns1:paragraph title="" name="">
        </ns1:paragraph>
    </ns1:chapter>

</ns1:book>
